A;AAEA,MAAA,cAAA,EAAA,KAFA;AAIA,MAAA,YAAA,EAAA;AACA,+BAAA,SADA;AAEA,kCAAA,CAAA,EAAA,EAAA,EAAA,EAAA,EAAA,EAAA,GAAA,EAAA,CAAA,CAAA,CAFA;AAGA,mCAAA;AAHA;AAJA,KAAA;AAAA,GAZA;AAuBA,EAAA,OAAA;AAAA,2EAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,qBAEA,KAAA,CAAA,GAAA,CAAA,KAAA,GAAA,UAAA,GAAA,KAAA,OAAA,CAFA;;AAAA;AAEA,cAAA,QAFA;AAGA,mBAAA,KAAA,GAAA,QAAA,CAAA,IAAA;AACA,mBAAA,KAAA,CAAA,MAAA,CAAA,IAAA,CAAA,UAAA,CAAA,EAAA,CAAA;AAAA,uBAAA,CAAA,CAAA,KAAA,GAAA,CAAA,CAAA,KAAA,GAAA,CAAA,GAAA,CAAA,CAAA;AAAA,eAAA;AACA,mBAAA,cAAA,GAAA,IAAA;AALA;AAAA;;AAAA;AAAA;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,KAAA;;AAAA;AAAA;AAAA;;AAAA;AAAA,KAvBA;AAmCA,EAAA,OAAA,EAAA;AAnCA,CAAA","sourcesContent":["<template>\n  <div>\n    <v-alert type=\"warning\" v-if=\"!filmeCarregado\">\n      A carregar informação\n    </v-alert>\n    <v-card class=\"ma-4\" v-else>\n      <v-card-title class=\"indigo darken-4 white--text\" dark>\n        <span class=\"headline\">Filme: \"{{ filme.info.titulo }}\" ({{idFilme}})</span>\n      </v-card-title>\n\n      <v-card-text>\n        <v-container>\n          <v-row>\n            <v-col cols=\"2\">\n              <div class=\"info-label\">Resumo</div>\n            </v-col>\n            <v-col>\n              <div class=\"info-content\">{{ filme.info.res }}</div>\n            </v-col>\n          </v-row>\n          <v-row>\n            <v-col cols=\"2\">\n              <div class=\"info-label\">Data</div>\n            </v-col>\n            <v-col>\n              <div class=\"info-content\">{{ filme.info.data }}</div>\n            </v-col>\n          </v-row>\n          <v-row>\n            <v-col cols=\"2\">\n              <div class=\"info-label\">Duração (minutos)</div>\n            </v-col>\n            <v-col>\n              <div class=\"info-content\">{{ filme.info.duracao }}</div>\n            </v-col>\n          </v-row>\n          <v-row>\n            <v-col cols=\"2\">\n              <div class=\"info-label\">Índice de Popularidade</div>\n            </v-col>\n            <v-col>\n              <div class=\"info-content\">{{ filme.info.pop }}</div>\n            </v-col>\n          </v-row>\n\n          <Linguas :lista=\"filme.linguas\" />\n          <Generos :lista=\"filme.generos\" />\n          <Atores :lista=\"filme.atores\" />\n          <Personagens :lista=\"filme.personagens\" />\n\n\n        </v-container>\n      </v-card-text>\n\n      <v-card-actions>\n        <v-spacer></v-spacer>\n        <v-btn color=\"blue darken-1\" text @click=\"$router.go(-1)\">voltar</v-btn>\n      </v-card-actions>\n    </v-card>\n\n  </div>\n</template>\n\n\n<script>\nimport axios from 'axios'\nconst lhost = require(\"@/config/global\").host;\n\nimport Linguas from \"@/components/filmes/Linguas.vue\"\nimport Generos from \"@/components/filmes/Generos.vue\"\nimport Personagens from \"@/components/filmes/Personagens.vue\"\nimport Atores from \"@/components/filmes/Atores.vue\"\n\nexport default {\n  name: 'Consulta filme',\n\n  components: {\n    Linguas, \n    Generos, \n    Atores, \n    Personagens\n  },\n\n  props: [\"idFilme\"],\n\n  data: () => ({\n    filme : [],\n    filmeCarregado : false,\n\n    footer_props: {\n      \"items-per-page-text\": \"Mostrar\",\n      \"items-per-page-options\": [10,20,50,100,-1],\n      \"items-per-page-all-text\": \"Todos\"\n    }\n  }),\n\n  created: async function(){\n    try {\n      let response = await axios.get(lhost + \"/filmes/\"+ this.idFilme);\n      this.filme = response.data\n      this.filme.atores.sort((a,b) => a.anome > b.anome ? 1 : -1)\n      this.filmeCarregado = true\n    } \n    catch (e) {\n      return e;\n    }\n  },\n\n  methods: {\n  }\n  \n}\n</script>\n\n<style>\n.info-label {\n  color: indigo;\n  padding: 5px;\n  font-weight: 400;\n  width: 100%;\n  background-color: #e0f2f1;\n  font-weight: bold;\n}\n\n.info-content {\n  padding: 5px;\n  width: 100%;\n  border: 1px solid #1a237e;\n}\n</style>"],"sourceRoot":"src/components"}]}