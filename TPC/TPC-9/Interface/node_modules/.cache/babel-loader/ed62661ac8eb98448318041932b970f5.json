,CAAA,CAFA;AAGA,mCAAA;AAHA;AAJA,KAAA;AAAA,GAVA;AAqBA,EAAA,OAAA;AAAA,2EAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,qBAEA,KAAA,CAAA,GAAA,CAAA,KAAA,GAAA,UAAA,GAAA,KAAA,MAAA,CAFA;;AAAA;AAEA,cAAA,QAFA;AAGA,mBAAA,IAAA,GAAA,QAAA,CAAA,IAAA;AACA,mBAAA,aAAA,GAAA,IAAA;AAJA;AAAA;;AAAA;AAAA;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,KAAA;;AAAA;AAAA;AAAA;;AAAA;AAAA,KArBA;AAgCA,EAAA,OAAA,EAAA;AAhCA,CAAA","sourcesContent":["<template>\n    <div>\n      <v-alert type=\"warning\" v-if=\"!atorCarregado\">\n        A carregar informação\n      </v-alert>\n\n      <v-card class=\"ma-4\" v-else>\n        <v-card-title class=\"indigo darken-4 white--text\" dark>\n          <span class=\"headline\">Ator: \"{{ ator.info.nome }}\" ({{idAtor}})</span>\n        </v-card-title>\n        <v-card-text>\n          <v-container>\n            <v-row>\n              <v-col cols=\"2\">\n                <div class=\"info-label\">Sexo</div>\n              </v-col>\n              <v-col>\n                <div class=\"info-content\">{{ ator.info.sex }}</div>\n              </v-col>\n            </v-row>\n\n            <Listagem \n              :lista=\"ator.produziu\" \n              nome=\"Produziu\"\n            />\n            <Listagem \n              :lista=\"ator.realizou\" \n              nome=\"Realizou\"\n            />\n            <Representou \n              :lista=\"ator.representou\" \n              nome=\"Representou\"\n            />\n            \n          </v-container>  \n        </v-card-text>\n\n        <v-card-actions>\n          <v-spacer></v-spacer>\n          <v-btn color=\"blue darken-1\" text @click=\"$router.go(-1)\">voltar</v-btn>\n        </v-card-actions>\n      </v-card>\n\n    </div>\n</template>\n\n\n<script>\nimport axios from 'axios'\nimport Listagem from \"@/components/atores/Listagem.vue\"\nimport Representou from \"@/components/atores/Representou.vue\"\nconst lhost = require(\"@/config/global\").host;\n\n\nexport default {\n  name: 'Consulta ator',\n\n  components: {\n    Listagem,\n    Representou\n  },\n\n  props: [\"idAtor\"],\n\n  data: () => ({\n    ator : [],\n    atorCarregado : false,\n\n    footer_props: {\n      \"items-per-page-text\": \"Mostrar\",\n      \"items-per-page-options\": [10,20,50,100,-1],\n      \"items-per-page-all-text\": \"Todos\"\n    }\n  }),\n\n  created: async function(){\n    try {\n      let response = await axios.get(lhost + \"/atores/\"+ this.idAtor);\n      this.ator = response.data\n      this.atorCarregado = true\n    } \n    catch (e) {\n      return e;\n    }\n  },\n\n  methods: {\n  }\n  \n}\n</script>\n\n<style>\n.info-label {\n  color: indigo;\n  padding: 5px;\n  font-weight: 400;\n  width: 100%;\n  background-color: #e0f2f1;\n  font-weight: bold;\n}\n\n.info-content {\n  padding: 5px;\n  width: 100%;\n  border: 1px solid #1a237e;\n}\n</style>"],"sourceRoot":"src/components"}]}